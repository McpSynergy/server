            },
            "genre": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "nasProps": {
              "type": "object",
              "properties": {
                "path": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "ext": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                }
              },
              "required": [
                "path",
                "name",
                "ext",
                "type"
              ]
            },
            "command": {
              "type": "string"
            }
          },
          "required": [
            "id",
