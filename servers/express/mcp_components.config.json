[
  {
    "name": "MediaCard",
    "description": "电影卡片组件，用于展示电影的基本信息并提供播放、收藏、分享等交互功能。",
    "serverName": "mcp-component-render",
    "propertySchema": {
      "type": "object",
      "properties": {
        "movie": {
          "type": "object",
          "properties": {
            "id": {
              "type": "number"
            },
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "poster": {
              "type": "string"
            },
            "year": {
              "type": "string"
            },
            "duration": {
              "type": "string"
            },
            "rating": {
              "type": "string"
            },
            "genre": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "nasProps": {
              "type": "object",
              "properties": {
                "path": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "ext": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                }
              },
              "required": [
                "path",
                "name",
                "ext",
                "type"
              ]
            },
            "command": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "title",
            "description",
            "poster",
            "year",
            "duration",
            "rating",
            "genre",
            "nasProps"
          ]
        }
      },
      "required": [
        "movie"
      ]
    },
    "inputSchema": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "description": "电影标题，必填字段。参数名必须保持为\"title\"，不得翻译为其他语言。title 不能包含 电影 字符串"
        },
        "command": {
          "type": "string",
          "enum": [
            "play",
            "open",
            "none"
          ],
          "description": "电影操作命令 enum, play: 播放电影,open: 打开电影,none: 查询或者搜索或者无操作命令",
          "default": "none"
        }
      },
      "required": [
        "title"
      ]
    }
  },
