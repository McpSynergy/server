{
  "version": 2,
  "buildCommand": "cd servers/express && pnpm install && pnpm build",
  "outputDirectory": "servers/express/dist",
  "installCommand": "pnpm install",
  "functions": {
    "servers/express/dist/main.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "servers/express/dist/main.js"
    },
    {
      "src": "/books/(.*)",
      "dest": "servers/express/dist/main.js"
    },
    {
      "src": "/message",
      "dest": "servers/express/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "servers/express/dist/main.js"
    }
  ]
} 