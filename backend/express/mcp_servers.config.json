{
    "mcp_servers": [
        {
            "enabled": false,
            "type": "stdio",
            "server_name": "nba-mcp-server",
            "command": "uvx",
            "args": [
                "nba-mcp-server"
            ]
        },
        {
            "enabled": false,
            "type": "stdio",
            "server_name": "server-puppeteer",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-puppeteer"
            ]
        },
        {
            "enabled": false,
            "type": "sse",
            "server_name": "server-everything-sse",
            "sse_url": "http://localhost:3001/sse"
        },
        {
            "enabled": false,
            "type": "stdio",
            "server_name": "github",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-github"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "<YOUR_TOKEN>"
            }
        },
         {
          "enabled": true,
          "type": "streamable_http",
          "server_name": "amap-maps-streamableHTTP",
          "http_url": "https://mcp.amap.com/mcp?key=034ef7ed1c93126e66363c9a5125e681"
        },
        {
           "enabled": true,
            "type": "stdio",
            "server_name": "mcp-component-render",
            "command": "node",
            "args": [
                "../mcpServers/component-render-mcp/dist/index.js"
            ]
        }
    ]
}
